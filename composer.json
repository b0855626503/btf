{
    "name": "gametech/website",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "alexmg86/laravel-sub-query": "^1.9",
        "alimranahmed/laraocr": "^1.2",
        "awobaz/compoships": "^2.0",
        "bacon/bacon-qr-code": "^2.0",
        "bagisto/bagisto-package-generator": "^2.0",
        "beyondcode/laravel-websockets": "^1.13",
        "codedge/laravel-selfupdater": "^3.2",
        "doctrine/dbal": "^3.6",
        "endroid/qr-code": "^5.0",
        "fideloper/proxy": "^4.3.0",
        "firebase/php-jwt": "^5.5",
        "fruitcake/laravel-cors": "^3.0",
        "google/apiclient": "2.12.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "infyomlabs/adminlte-templates": "^3.0",
        "infyomlabs/laravel-generator": "^3.0",
        "irazasyed/telegram-bot-sdk": "^3.9",
        "jaybizzle/laravel-crawler-detect": "^1.2",
        "jenssegers/agent": "^2.6",
        "jenssegers/mongodb": "^3.8",
        "klevze/online-users": "^0.1.2",
        "konekt/concord": "^1.8",
        "laravel/framework": "^8.0",
        "laravel/horizon": "^5.7",
        "laravel/octane": "^1.5",
        "laravel/scout": "^8.3",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "maatwebsite/excel": "^3.1",
        "paquettg/php-html-parser": "^3.1",
        "php-curl-class/php-curl-class": "^9.1",
        "pragmarx/google2fa-laravel": "^1.4",
        "predis/predis": "^1.1",
        "prettus/l5-repository": "^2.7",
        "pusher/pusher-php-server": "^7.2",
        "rap2hpoutre/fast-excel": "^2.5",
        "silviolleite/laravelpwa": "^2.0",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spiritix/lada-cache": "^5.1",
        "voku/simple_html_dom": "^4.7",
        "yajra/laravel-datatables": "1.5",
        "yajra/laravel-datatables-oracle": "~9.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.22",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3",
        "kitloong/laravel-migrations-generator": "^4.4"
    },
    "config": {
        "optimize-autoloader": true,
        "classmap-authoritative": false,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform-check": false,
        "platform": {
            "ext-pcntl": "8.1",
            "ext-posix": "8.1"
        },
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk",
                "facade/ignition",
                "mockery/mockery",
                "nunomaduro/collision",
                "phpunit/phpunit",
                "barryvdh/laravel-debugbar"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Gametech\\Admin\\": "packages/Gametech/Admin/src",
            "Gametech\\Auto\\": "packages/Gametech/Auto/src",
            "Gametech\\Core\\": "packages/Gametech/Core/src",
            "Gametech\\Member\\": "packages/Gametech/Member/src",
            "Gametech\\API\\": "packages/Gametech/API/src",
            "Gametech\\Game\\": "packages/Gametech/Game/src",
            "Gametech\\Promotion\\": "packages/Gametech/Promotion/src",
            "Gametech\\Ui\\": "packages/Gametech/Ui/src",
            "Gametech\\Payment\\": "packages/Gametech/Payment/src",
            "Gametech\\Wallet\\": "packages/Gametech/Wallet/src",
            "Gametech\\LogAdmin\\": "packages/Gametech/LogAdmin/src",
            "Gametech\\LogUser\\": "packages/Gametech/LogUser/src",
            "Gametech\\TelegramBot\\": "packages/Gametech/TelegramBot/src",
            "Gametech\\Marketing\\": "packages/Gametech/Marketing/src",
            "Gametech\\Integrations\\": "packages/Gametech/Integrations/src"
        },
        "files": [
            "packages/Gametech/Core/src/Http/helpers.php",
            "packages/Gametech/Admin/src/Http/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
